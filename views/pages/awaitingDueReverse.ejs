<!-- views/pages/index.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../partials/head %>
</head>
<body class="container">

<header>
    <% include ../partials/header %>
</header>

<main>
    <div class="jumbotron">
        <h3>Awaiting Fulfillment</h3>

    <table>
      <tr>
        <th>Order #</th>
        <th><a href="/awaitingReverse">Order Placed</a></th>
        <th><a href="/awaitingDue">Order Due</a></th>
        <th>Overdue</th>
      </tr>
            <% dummy.forEach(function(dummy) { %>
             <tr>
                        <td><%= dummy.order %></td>
                        <td><%= dummy.placed %></td>
                        <td><%= dummy.due %></td>
                        <td><%= dummy.overdue %></td>
             </tr>
            <% }); %>
        </table>
    </div>
</main>


<footer>
    <% include ../partials/footer %>
</footer>

</body>
</html>